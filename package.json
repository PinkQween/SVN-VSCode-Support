{
  "name": "swift-visual-novels",
  "displayName": "Swift Visual Novels Language",
  "description": "Language support for Swift Visual Novels",
  "publisher": "pinkqween",
  "homepage": "https://github.com/PinkQween/SVN-VSCode-Support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinkqween/SVN-VSCode-Support.git"
  },
  "icon": "./SVN.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "swiftVisualNovels",
        "extensions": [
          ".svn"
        ],
        "aliases": [
          "Swift Visual Novels"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./SVN.png",
          "dark": "./SVN.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "swiftVisualNovels",
        "scopeName": "source.swiftVisualNovels",
        "path": "./syntaxes/swiftVisualNovels.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "swiftVisualNovels",
        "path": "./snippets.json"
      }
    ]
  }
}