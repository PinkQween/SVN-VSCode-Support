{
    "comments": {
        "lineComment": "//",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "\"",
            "close": "\"",
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "[",
            "close": "]"
        }
    ],
    "surroundingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "[",
            "close": "]"
        }
    ],
    "strings": {
        "tokenizeEscapeSequences": true,
        "escapeSequences": [
            "\\\\",
            "\\\"",
        ]
    },
    "keywords": [
        "var",
        "let",
        "const"
    ],
}